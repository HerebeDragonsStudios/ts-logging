{
  "name": "ts-logging",
  "version": "0.0.1",
  "description": "Logging extension for Typescript/Javascript projects",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "publishConfig": {
    "@glass-project1:registry": " https://gitlab.com/api/v4/projects/39103644/packages/npm/"
  },
  "scripts": {
    "start:dev": "nodemon",
    "start": "npm run build && node lib/index.js",
    "build": "rimraf ./lib && rimraf ./dist && gulp dev",
    "build:prod": "rimraf ./lib && rimraf ./dist && gulp prod",
    "test": "jest --coverage --testPathPattern=\"/tests/unit\"",
    "test:integration": "jest --coverage --testPathPattern=\"/tests/(integration)\" --passWithNoTests --detectOpenHandles",
    "test:all": "jest --coverage --testPathPattern=\"/tests/(unit|integration)\" --passWithNoTests --detectOpenHandles",
    "prepare-release": "npm run build:prod && npm run test && npm run docs",
    "release": "./bin/tag-release.sh",
    "clean-publish": "npx clean-publish",
    "coverage": "npm test && jest-coverage-badges --input \"./workdocs/coverage/coverage-summary.json\" --output \"./workdocs/badges\"",
    "docs": "npx rimraf ./docs && mkdir docs && mkdir docs/assets && cp -r ./workdocs/assets ./docs/assets && npm install better-docs taffydb && npx markdown-include ./mdCompile.json && npx jsdoc -c jsdocs.json -t ./node_modules/better-docs; npm remove better-docs taffydb"
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HereBeDragonsStudios/ts-logging.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "typescript",
    "decorators",
    "logging",
    "timestamp"
  ],
  "author": "Here Be Dragons Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HereBeDragonsStudios/ts-logging/issues"
  },
  "homepage": "https://github.com/HereBeDragonsStudios/ts-logging#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.2",
    "jsdoc-mermaid": "^1.0.0",
    "markdown-include": "^0.4.3",
    "merge-stream": "^2.0.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
